<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/converterFragment">
    <fragment
        android:id="@+id/converterFragment"
        android:name="org.d3if3068.assesment001.ui.Converter.ConverterFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_converter">
        <action
            android:id="@+id/action_converterFragment_to_helpFragment"
            app:destination="@id/helpFragment" />
        <action
            android:id="@+id/action_hitungFragment_to_historiFragment"
            app:destination="@id/historiFragment" />
    </fragment>
    <fragment
        android:id="@+id/helpFragment"
        android:name="org.d3if3068.assesment001.ui.help.HelpFragment"
        android:label="@string/Help"
        tools:layout="@layout/fragment_help" />
    <fragment
        android:id="@+id/historiFragment"
        android:name="org.d3if3068.assesment001.ui.histori.HistoriFragment"
        android:label="@string/histori"
        tools:layout="@layout/fragment_histori" />

</navigation>